<html>
    <body>
        <table border="1px">
            <tr>
                <td>Id</td>
                <td>Título</td>
                <td>Descrição</td>
                <td>Preço</td>
                <td colspan="2">Ações</td>
            </tr>
            <%for(var i=0; i<lista.length; i++) {%>
                <tr>
                    <td><%=lista[i].id%></td>
                    <td><%=lista[i].titulo%></td>
                    <td><%=lista[i].descricao%></td>
                    <td><%=lista[i].preco%></td>
                    <td><input type="button" value="Editar" /></td>
                    <td><input type="button" value="Remover" /></td>
                </tr>
            <%}%>
        </table>
    </body>
</html>